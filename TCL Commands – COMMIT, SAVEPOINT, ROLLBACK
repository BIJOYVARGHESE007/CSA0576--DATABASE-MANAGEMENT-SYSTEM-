CREATE TABLE employees (
    emp_id INT PRIMARY KEY,
    emp_name VARCHAR(50),
    salary INT
);
INSERT INTO employees VALUES (1, 'Sherly', 30000);
INSERT INTO employees VALUES (2, 'Kishore', 35000);
INSERT INTO employees VALUES (3, 'Ravi', 40000);
SELECT * FROM employees;
SAVEPOINT point1;
UPDATE employees SET salary = 50000 WHERE emp_id = 2;
SELECT * FROM employees;

SAVEPOINT point2;

DELETE FROM employees WHERE emp_id = 3;
SELECT * FROM employees;

ROLLBACK TO point1;
SELECT * FROM employees;

COMMIT;
